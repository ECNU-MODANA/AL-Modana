<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.1//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_2.dtd'>
<nta>
	<declaration>// Place global declarations here.
broadcast chan off[2],on[2];
clock T0,T1;</declaration>
	<template>
		<name x="5" y="5">Heater</name>
		<declaration>// Place local declarations here.
clock x;</declaration>
		<location id="id0" x="102" y="17">
			<urgent/>
		</location>
		<location id="id1" x="93" y="-170">
			<urgent/>
		</location>
		<location id="id2" x="-59" y="17">
			<name x="-76" y="-17">ON_1</name>
			<label kind="exponentialrate" x="-59" y="34">1</label>
		</location>
		<location id="id3" x="-59" y="-161">
			<name x="-69" y="-195">ON_0</name>
			<label kind="exponentialrate" x="-59" y="-144">2</label>
		</location>
		<location id="id4" x="-229" y="-59">
			<name x="-239" y="-93">OFF</name>
			<label kind="invariant" x="-239" y="-42">x&lt;=4</label>
		</location>
		<branchpoint id="id5" x="-59" y="-68">
		</branchpoint>
		<init ref="id4"/>
		<transition>
			<source ref="id0"/>
			<target ref="id2"/>
			<label kind="synchronisation" x="-17" y="-8">on[1]!</label>
		</transition>
		<transition>
			<source ref="id1"/>
			<target ref="id3"/>
			<label kind="synchronisation" x="-8" y="-187">on[0]!</label>
		</transition>
		<transition>
			<source ref="id5"/>
			<target ref="id0"/>
			<label kind="probability" x="17" y="-51">3</label>
		</transition>
		<transition>
			<source ref="id5"/>
			<target ref="id1"/>
			<label kind="probability" x="-8" y="-127">1</label>
		</transition>
		<transition>
			<source ref="id4"/>
			<target ref="id5"/>
		</transition>
		<transition>
			<source ref="id2"/>
			<target ref="id4"/>
			<label kind="synchronisation" x="-153" y="-42">off[1]!</label>
			<label kind="assignment" x="-144" y="-17">x=0</label>
		</transition>
		<transition>
			<source ref="id3"/>
			<target ref="id4"/>
			<label kind="synchronisation" x="-161" y="-153">off[0]!</label>
			<label kind="assignment" x="-153" y="-136">x=0</label>
		</transition>
	</template>
	<template>
		<name>Room0</name>
		<declaration>clock K;</declaration>
		<location id="id6" x="-17" y="-17">
			<name x="-27" y="-51">ON</name>
			<label kind="invariant" x="-27" y="0">T0'==K-T0/10&amp;&amp;
K'==0</label>
		</location>
		<location id="id7" x="-187" y="-25">
			<name x="-238" y="-51">OFF</name>
			<label kind="invariant" x="-272" y="-34">T0'==-T0/10</label>
		</location>
		<location id="id8" x="-187" y="-136">
			<name x="-197" y="-170">Init</name>
			<committed/>
		</location>
		<init ref="id8"/>
		<transition>
			<source ref="id6"/>
			<target ref="id7"/>
			<label kind="synchronisation" x="-119" y="17">off[0]?</label>
			<nail x="-68" y="17"/>
			<nail x="-127" y="17"/>
		</transition>
		<transition>
			<source ref="id7"/>
			<target ref="id6"/>
			<label kind="synchronisation" x="-110" y="-102">on[0]?</label>
			<label kind="assignment" x="-144" y="-85">K=9+random(3)</label>
			<nail x="-161" y="-42"/>
			<nail x="-102" y="-59"/>
		</transition>
		<transition>
			<source ref="id8"/>
			<target ref="id7"/>
			<label kind="assignment" x="-255" y="-102">T0=0</label>
		</transition>
	</template>
	<template>
		<name>Room1</name>
		<declaration>clock K;</declaration>
		<location id="id9" x="51" y="110">
			<name x="41" y="76">ON</name>
			<label kind="invariant" x="41" y="127">T1'==K-T1/10&amp;&amp;
K'==0</label>
		</location>
		<location id="id10" x="-119" y="102">
			<name x="-170" y="76">OFF</name>
			<label kind="invariant" x="-212" y="93">T1'==-T1/10</label>
		</location>
		<location id="id11" x="-119" y="-9">
			<name x="-129" y="-43">Init</name>
			<committed/>
		</location>
		<init ref="id11"/>
		<transition>
			<source ref="id9"/>
			<target ref="id10"/>
			<label kind="synchronisation" x="-59" y="153">off[0]?</label>
			<nail x="17" y="127"/>
			<nail x="-29" y="166"/>
			<nail x="-89" y="157"/>
		</transition>
		<transition>
			<source ref="id10"/>
			<target ref="id9"/>
			<label kind="synchronisation" x="-42" y="25">on[1]?</label>
			<label kind="assignment" x="-76" y="42">K=9+random(3)</label>
			<nail x="-93" y="85"/>
			<nail x="-34" y="68"/>
		</transition>
		<transition>
			<source ref="id11"/>
			<target ref="id10"/>
			<label kind="assignment" x="-195" y="25">T1=0</label>
		</transition>
	</template>
	<system>// Place template instantiations here.
// List one or more processes to be composed into a system.
system Heater,Room0,Room1;
    </system>
	<queries>
		<query>
			<formula>simulate 1 [&lt;=120] {T0,T1,Heater.OFF,Heater.ON_0,Heater.ON_1,Room0.OFF,Room0.ON,Room1.OFF,Room1.ON}
			</formula>
			<comment>
			</comment>
		</query>
	</queries>
</nta>
